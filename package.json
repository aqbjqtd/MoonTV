{
  "name": "moontv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm gen:manifest && pnpm gen:runtime && next dev -H 0.0.0.0",
    "build": "pnpm gen:manifest && pnpm gen:runtime && next build",
    "start": "next start",
    "lint": "pnpm lint:types && pnpm lint:format",
    "lint:fix": "pnpm format",
    "lint:types": "tsc --noEmit --incremental false",
    "lint:format": "prettier --check .",
    "lint:strict": "pnpm lint:types && pnpm format:check",
    "lint:next": "next lint",
    "typecheck": "tsc --noEmit --incremental false",
    "test:watch": "jest --watch",
    "test": "jest",
    "format": "prettier -w .",
    "format:check": "prettier -c .",
    "gen:manifest": "node scripts/generate-manifest.js",
    "gen:runtime": "node scripts/generate-runtime.js",
    "postbuild": "echo 'Build completed - sitemap generation disabled'",
    "prepare": "husky install",
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@upstash/redis": "^1.25.0",
    "@vidstack/react": "^1.12.13",
    "artplayer": "^5.2.5",
    "clsx": "^2.0.0",
    "framer-motion": "^12.18.1",
    "glob-to-regexp": "^0.4.1",
    "he": "^1.2.0",
    "hls.js": "^1.6.6",
    "lucide-react": "^0.438.0",
    "media-icons": "^1.1.5",
    "next": "14.2.32",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0",
    "redis": "^4.6.7",
    "sweetalert2": "^11.11.0",
    "swiper": "^11.2.8",
    "tailwind-merge": "^2.6.0",
    "vidstack": "^0.6.15",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/forms": "^0.5.10",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^15.0.7",
    "@types/aria-query": "^5.0.4",
    "@types/he": "^1.2.3",
    "@types/node": "24.0.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^19.1.6",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.32",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-react": "^7.37.5",
    "husky": "^7.0.4",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^12.5.0",
    "next-router-mock": "^0.9.0",
    "postcss": "^8.5.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "semver": "^7.7.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "next lint",
      "prettier -w"
    ],
    "**/*.{json,css,scss,md,webmanifest}": [
      "prettier -w"
    ]
  },
  "packageManager": "pnpm@10.14.0"
}
